{{define "projectLinks"}}
<table>
    <tr>
        <td></td>
        {{if .prev}}
        <td><h3><a href='{{.prev_url}}'>&lt&nbsp{{.prev}}</a></h3></td>
        {{end}}
        {{if .next}}
        <td><h3 align="right"><a href='{{.next_url}}'>{{.next}} &gt</a></h3></td>
        {{end}}
    </tr>
</table>
{{end}}
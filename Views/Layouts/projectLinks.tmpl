{{define "projectLinks"}}
<div id="next-prev">
    <div>
            {{if .prev}}
            <h3><a href='{{.prev_url}}'>&lt&nbsp{{.prev}}</a></h3>
            {{end}}
    </div>
    <div>
        {{if .next}}
        <h3 align="right"><a href='{{.next_url}}'>{{.next}} &gt</a></h3>
        {{end}}
    </div>
</div>
{{end}}